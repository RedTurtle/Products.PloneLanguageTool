#: ./LanguageTool/prefs_languages.pt
#. <p i18n:translate="decription_content_settings">
#.             Check the settings that apply to multilingual content.
#. </p>
# Original: "Check the settings that apply to multilingual content."
msgid "decription_content_settings"
msgstr "Setzen Sie die Einstellungen, die auf mehrsprachigen Inhalt zutreffen."


#: ./LanguageTool/prefs_languages.pt
#. <p i18n:translate="description_allowed_languages">
#.             Select the languages that can be added in your portal.
#. </p>
# Original: "Select the languages that can be added in your portal."
msgid "description_allowed_languages"
msgstr "Wählen Sie die Sprachen aus, in denen Artikel zu Ihrer Website hinzugefügt werden können sollen."


#: ./LanguageTool/prefs_languages.pt
#. <p i18n:translate="description_default_language">
#.             If content requested is not available in the language the user
#.             preferred, content will be presented in the fallback language.
#. </p>
# Original: "If content requested is not available in the language the user preferred, content will be presented in the fallback language."
msgid "description_default_language"
msgstr "Sollte der Artikel nicht in der vom Benutzer gewünschten Sprache zur Verfügung stehen, wird der Artikel in der als Ausweichlösung gewählten Sprache angezeigt."


#: ./LanguageTool/prefs_languages.pt
#. <p i18n:translate="description_negotiation_scheme">
#.             Check the language negotiation schemes that apply to this site.
#. </p>
# Original: "Check the language negotiation schemes that apply to this site."
msgid "description_negotiation_scheme"
msgstr "Wählen Sie die Sprachauswahlschemata die diese Website zur Verfügung stellen soll."


#: ./LanguageTool/prefs_languages.pt
#. <h2 i18n:translate="heading_allowed_languages">
#.  Allowed languages
#. </h2>
# Original: "Allowed languages"
msgid "heading_allowed_languages"
msgstr "Erlaubte Sprachen"


#: ./LanguageTool/prefs_languages.pt
#. <h2 i18n:translate="heading_content_settings">
#.  Content Control Settings
#. </h2>
# Original: "Content Control Settings"
msgid "heading_content_settings"
msgstr "Inhaltskontrolleinstellungen"


#: ./LanguageTool/prefs_languages.pt
#. <h2 i18n:translate="heading_default_language">
#.  Default language
#. </h2>
# Original: "Default language"
msgid "heading_default_language"
msgstr "Voreingestellte Sprache"


#: ./LanguageTool/prefs_languages.pt
#. <h2 i18n:translate="heading_negotiation_scheme">
#.  Negotiation Scheme
#. </h2>
# Original: "Negotiation Scheme"
msgid "heading_negotiation_scheme"
msgstr "Verhandlungsschema"


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_allow_combined_language_codes">
#.                 Allow combined language codes like de-de or en-uk.
#. </span>
# Original: "Allow combined language codes like de-de or en-uk."
msgid "label_allow_combined_language_codes"
msgstr "Erlaube kombinierte Sprachkodes, wie z.B. de-de oder en-uk."


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_allow_content_language_fallback">
#.                     Allow content language fallback if the implementation supports it.
#. </span>
# Original: "Allow content language fallback if the implementation supports it."
msgid "label_allow_content_language_fallback"
msgstr "Erlaube Inhalt auf eine andere Sprache auszuweichen, sollte dieses von der Implementierung unterstützt werden."


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_browser_language_negotiation">
#.                     Use browser language request negotiation.
#. </span>
# Original: "Use browser language request negotiation."
msgid "label_browser_language_negotiation"
msgstr "Benutze Aushandlung der Sprache durch den Browser."


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_cookies_for_override">
#.                     Use cookie for manual override.
#. </span>
# Original: "Use cookie for manual override."
msgid "label_cookies_for_override"
msgstr "Benutze Cookie für manuelle Übersteuerung"


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_display_flags">
#.                 Display flags for language selection (May be politically
#.                 sensitive in some areas).
#. </span>
# Original: "Display flags for language selection (May be politically sensitive in some areas)."
msgid "label_display_flags"
msgstr "Zeige Flaggen für Sprachwahl (Dieses ist in einigen Gegenden politisch sensibel)."


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_force_different_urls">
#.                     Force different urls for each language (redirect).
#. </span>
# Original: "Force different urls for each language (redirect)."
msgid "label_force_different_urls"
msgstr "Erzwinge unterschiedliche URL's für jede Sprache (Umleitung)."


#: ./LanguageTool/prefs_languages.pt
#. <span i18n:translate="label_language_codes_in_url">
#.                     Use language codes in URL path for manual override.
#. </span>
# Original: "Use language codes in URL path for manual override."
msgid "label_language_codes_in_url"
msgstr "Benutze Sprachkodes in der URL für manuelle Übersteuerung."


#: ./LanguageTool/prefs_languages.pt
#. <li i18n:translate="text_default_fallback">
#.                 Default fallback (always enabled). This is the language
#.                 specified as default above.
#. </li>
# Original: "Default fallback (always enabled). This is the language specified as default above."
msgid "text_default_fallback"
msgstr "Voreingestellte Ausweichsprache (immer aktiv). Dieses ist die Sprache die weiter oben als voreingestellt definiert wurde."


#: ./LanguageTool/prefs_languages.pt
#. <p class="discreet" i18n:translate="text_select_languages">
#.             Use the Ctrl or Apple key to select / deselect single items.
#. </p>
# Original: "Use the Ctrl or Apple key to select / deselect single items."
msgid "text_select_languages"
msgstr "Benutzen Sie Strg- oder die Apple-Taste um einzelne Einträge auszuwählen."

